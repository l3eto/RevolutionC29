    <lyrics>
      <lyric name="intro" chord="1">
      </lyric>
      <lyric name="verse1" chord="2">
        <p text="Tu voz me llama a las aguas"></p>
        <p text="donde mis pies pueden fallar"></p>
        <p text="y ahí te encuentro en lo incierto"></p>
        <p text="caminaré sobre el mar"></p>
      </lyric>
      <lyric name="verse2" chord="2">
        <p text="Tu gracia abunda en la tormenta"></p>
        <p text="tu mano Dios, me guiará"></p>
        <p text="cuando hay temor en mi camino"></p>
        <p text="Tú eres fiel y no cambiarás"></p>
      </lyric>
      <lyric name="chorus" chord="3">
        <p text="Y a tu nombre clamaré"></p>
        <p text="en ti mis ojos fijaré"></p>
        <p text="en tempestad, descansaré en tu poder"></p>
        <p text="pues tuyo soy hasta el final"></p>
      </lyric>
