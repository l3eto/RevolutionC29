      <song name="La Experiencia de Adorar" id="ueu6r8jjor" order="intro-verse1-verse2-chorus1-intro-verse1-verse2-chorus1-chorus2-bridge-bridge-bridge-chorus1-chorus2-intro">
        <chords>
          <chord name="1">
            <p><a temp="4" note="C"></a><a temp="4" note="G"></a><a temp="4" note="Am"></a><a temp="4" note="F"></a></p>
          </chord>
          <chord name="2">
